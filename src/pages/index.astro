---
import "../css/global.css";
import Forsideheader from "../components/Forsideheader.astro";
import Layout from "../layouts/Layout.astro";
import About from "../components/About.astro";
import Indexproducts from "../components/Indexproducts.astro";
import Quote_lp from "../components/Quote_lp.astro";
---

<Forsideheader />
<!-- <a href="#"><span>GET TICKETS</span></a> -->
<Layout title="Kune">
  <Indexproducts />
  <Quote_lp />

  <style>
    .grid_1-1 {
      display: grid;
      grid-template-columns: 1fr 1fr;
      padding: 2% 0 2% 0;
    }

    .forside_img {
      box-sizing: border-box;
      margin: auto;
      width: 494px;
      height: 330px;

      background: url("../assets/kune_ungdomsoe.webp");
      border: 3px solid #000000;
      border-radius: 30px 0px;
      background-size: cover;
    }

    p {
      margin: auto;
      width: 551px;
      height: 330px;

      font-family: "Fugue";
      font-style: normal;
      font-weight: 400;
      font-size: 25px;
      line-height: 30px;
      display: flex;
      justify-content: center;

      color: #000000;
    }
  </style>
</Layout>
